{
    "name": "martin-kozianka/contao-user2group-bundle",
    "description": "User 2 Group Viewer, Group and user definition summary page for the backend",
    "keywords": [
        "contao",
        "user",
        "groups",
        "summary",
        "backend"
    ],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Martin Kozianka",
            "email": "martin@kozianka.de",
            "homepage": "http://kozianka.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "martin@kozianka.de",
        "issues": "https://github.com/kozi/contao-user2group/issues",
        "source": "https://github.com/kozi/contao-user2group"
    },
    "type": "contao-bundle",
    "autoload": {
        "psr-4": {
            "MartinKozianka\\User2GroupBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao/"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    },
    "require": {
        "php": ">=7.2.0",
        "contao/core-bundle": "~4.4"
    },
    "extra": {
        "contao-manager-plugin": "MartinKozianka\\User2GroupBundle\\ContaoManager\\Plugin"
    }
}